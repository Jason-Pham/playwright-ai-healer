# Environment Configuration Template
# Copy this file to .env, .env.dev, .env.staging, or .env.prod and fill in values

# Environment name (dev, staging, prod)
ENV=dev

# Application URLs
BASE_URL=https://www.gigantti.fi/

# AI Provider Configuration
# Choose provider: 'gemini' or 'openai'
AI_PROVIDER=gemini

# Gemini API
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-flash-latest

# OpenAI API (supports comma-separated list for key rotation)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_KEYS=key1,key2,key3
OPENAI_MODEL=gpt-4o

# Logging
LOG_LEVEL=info

# Test Configuration
TEST_TIMEOUT=120000
HEADLESS=true
