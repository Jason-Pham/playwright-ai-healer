{
  "name": "self-healing-agent",
  "version": "1.0.0",
  "description": "Playwright self-healing test automation framework",
  "main": "index.js",
  "scripts": {
    "test:dev": "cross-env TEST_ENV=dev playwright test --project=dev",
    "test:staging": "cross-env TEST_ENV=staging playwright test --project=staging",
    "test:prod": "cross-env TEST_ENV=prod playwright test --project=prod",
    "test:prod:all-browsers": "cross-env TEST_ENV=prod playwright test --project=prod --project=chromium --project=chrome --project=firefox --project=webkit --project=edge --project=mobile-chrome --project=mobile-safari --project=tablet",
    "test:all-envs": "npm run test:dev && npm run test:staging && npm run test:prod",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "cross-env TEST_ENV=prod playwright test --project=webkit",
    "test:all-browsers": "playwright test --project=default --project=firefox --project=webkit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "playwright",
    "self-healing",
    "test-automation",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.10",
    "dotenv": "^17.2.3",
    "openai": "^6.16.0",
    "playwright": "^1.58.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^7.0.3",
    "vitest": "^4.0.18"
  }
}